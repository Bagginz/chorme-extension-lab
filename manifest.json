{
    "manifest_version": 3,
    "name": "Copy HTML Code",
    "description": "Base Level Extension",
    "version": "1.0",
    "icons": {
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}